<%- include('includes/header'); %>
<%- include('includes/nav'); %>

    <div class="container">
      <div class="row">
        <div class="col-lg-2"></div>

        <div class="col-lg-8 my-3">
          <h1 class="text-center">Briefing Semanal</h1>
          <p class="text-center lead">Lista de Briefing Semanais</p>

          <%- include('includes/messages'); %>

          <table id="tabelaBriefings">
            <thead>
                <tr>
                    <td>Titulo</td>
                    <td>Assunto</td>
                    <td>Ações</td>
                </tr>
            </thead>
            <tbody>
              <% briefings.forEach( briefing => { %>
                <tr>
                  <td><%= briefing.titulo %></td>
                  <td><%= briefing.assunto %></td> 
                  <td>
                    <a href="">Alguma coisa</a>
                    <a href="">Outra coisa</a>
                  </td> 
                </tr>         
              <% }); %>
            </tbody>
          </table>
         
        </div>

        <div class="col-lg-2"></div>
      </div>
    </div>

<%- include('includes/footer'); %>